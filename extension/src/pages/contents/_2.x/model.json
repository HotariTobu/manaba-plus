{
    "selectors": ".course-cell a:first-of-type, .courselist-title a, .course-card-title a",
    "children": [
        {
            "key": "news",
            "prefix": "_news",
            "selectors": ".newstext a",
            "children": [
                {
                    "selectors": ".file + .inlineaf-description a"
                }
            ]
        },
        {
            "key": "report",
            "prefix": "_report",
            "selectors": ".report-title a",
            "children": [
                {
                    "selectors": ".attachments a"
                },
                {
                    "key": "assignments",
                    "selectors": "a[href*=\"collectiondetail\"]",
                    "children": [
                        {
                            "selectors": ".contentslist a",
                            "children": [
                                {
                                    "selectors": ".report-submit-list a:not(.zoom-off)"
                                }
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "key": "page",
            "prefix": "_page",
            "selectors": ".about-contents a",
            "children": [
                {
                    "selectors": ".contentslist a",
                    "children": [
                        {
                            "selectors": ".file + .inlineaf-description a"
                        }
                    ]
                }
            ]
        }
    ]
}